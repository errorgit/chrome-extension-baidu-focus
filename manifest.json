{
    "manifest_version": 2,
    "name": "search helper",
    "version": "1.0.0",
    "description": "drop mouse",
    "icons":
    {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "permissions": [
        "https://www.baidu.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.baidu.com/*"],
            "js": ["js/search_key.js"],
            "run_at": "document_end"
        }
    ]
}
